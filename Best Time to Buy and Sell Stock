class Solution {
    public int maxProfit(int[] prices) {
        int minPrice = Integer.MAX_VALUE;
        int maxProfit = 0;
        
        for (int p : prices) {
            minPrice = Math.min(minPrice, p);
            maxProfit = Math.max(maxProfit, p - minPrice);
        }
        
        return maxProfit;
    }
}
Output :
Case1 :
Prices = [7,1,5,3,6,4]
Case2 :
Prices = [7,6,4,3,1]
